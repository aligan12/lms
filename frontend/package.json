	"main": "index.js",
	"scripts": {
		"start": "webpack serve --env port=3000",
		"move_locales": "npx shx mkdir -p locales/en locales/ru  && npx shx mv ./locales ../backend/lmssite/frontend/build/static && npx shx cp -r ./public/locales/* ../backend/lmssite/frontend/build/static/locales/",
		"build:export": "webpack --env mode=production --output-path ../backend/lmssite/frontend/build && npm run move_locales ",
		"move_locales_build": "npx shx mkdir -p ./build/locales/en ./build/locales/ru  && npx shx cp -r ./public/locales/* ./build/locales/",
		"build:azure": "webpack --env mode=production && npm run move_locales_build",
		"build:prod": "webpack --env mode=production",
		"build:dev": "webpack --env mode=development   && npm run move_locales_build",
		"eslint:fix": "npx eslint '**/*.{ts,tsx}'--fix",
		"eslint": "npx eslint \"**/*.{ts,tsx}\"",
		"prettier": "npx prettier --check \"**/*.{ts,tsx}\"",
		"prettier:fix": "npx prettier --write \"**/*.{ts,tsx}\"",
		"stylelint": "npx stylelint \"**/*.scss\"",
		"stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
		"jest": "jest --config ./config/jest/jest.config.ts",
		"storybook": "storybook dev -p 6006 -c ./config/storybook",
		"build-storybook": "storybook build -c ./config/storybook",
		"prepare": "cd .. && husky install frontend/.husky"
	},
